# This exercise has more printing exercises.
# Amith, 01/02/2015

print "Mary had a little lamb."
print "Its fleece was white as %s." % 'snow'
print "And everywhere that Mary went."
print '.' * 10 # this prints the '.' ten times.

end1 = 'C'
end2 = 'h'
end3 = 'e'
end4 = end3
end5 = 's'
end6 = ' '
end7 = 'B'
end8 = 'u'
end9 = 'r'
end10 = 'g'
end11 = end3
end12 = end9

# Watch that comma at the end. It does something fancy!
print end1 + end2 + end3 + end4 + end5 + end6, # note the comma here!
print end7 + end8 + end9 + end10 + end11 + end12

#without the comma.
print end1 + end2 + end3 + end4 + end5 + end6
print end7 + end8 + end9 + end10 + end11 + end12
